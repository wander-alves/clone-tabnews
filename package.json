  "main": "./pages/index.js",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "services:up": "docker compose -f ./cursodev/compose.yaml up -d",
    "services:stop": "docker compose -f ./cursodev/compose.yaml stop",
    "services:down": "docker compose -f ./cursodev/compose.yaml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate -m ./infra/migrations/ create",
    "migreation:up": "node-pg-migrate --envPath ./.env.development up"
  },
